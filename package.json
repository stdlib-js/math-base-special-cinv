        "desc": "result",
        "type": {
          "javascript": "Complex128",
          "jsdoc": "Complex128",
          "c": "stdlib_complex128_t",
          "dtype": "complex128"
        }
      },
      "keywords": [
        "cinv",
        "inv",
        "inverse",
        "reciprocal",
        "complex",
        "cmplx"
